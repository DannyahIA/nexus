{
  "expo": "51.0.0",
  "name": "nexus",
  "slug": "nexus",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./app/assets/icon.png",
  "userInterfaceStyle": "dark",
  "splash": {
    "image": "./app/assets/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#000000"
  },
  "assetBundlePatterns": [
    "**/*"
  ],
  "ios": {
    "supportsTabletMode": true,
    "bundleIdentifier": "com.nexus.app"
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./app/assets/adaptive-icon.png",
      "backgroundColor": "#000000"
    },
    "package": "com.nexus.app"
  },
  "web": {
    "favicon": "./app/assets/favicon.png"
  },
  "plugins": [
    [
      "expo-build-properties",
      {
        "ios": {
          "newArchEnabled": true
        },
        "android": {
          "newArchEnabled": true
        }
      }
    ],
    [
      "expo-camera",
      {
        "cameraPermission": "Allow Nexus to access your camera.",
        "microphonePermission": "Allow Nexus to access your microphone."
      }
    ],
    "react-native-webrtc"
  ],
  "extra": {
    "eas": {
      "projectId": "nexus-project-id"
    }
  }
}
